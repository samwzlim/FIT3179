{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "background": "white",
  "padding": 5,
  "height": 200,
  "title": {"text": "Minimum Wage Distribution", "frame": "group"},
  "style": "cell",
  "data": [
    {
      "name": "source_0",
      "url": "https://raw.githubusercontent.com/samwzlim/FIT3179/main/visualisation2/data/minimum_wage_over_years.csv",
      "format": {
        "type": "csv",
        "parse": {"Year": "date:'%Y'"},
        "delimiter": ","
      },
      "transform": [
        {
          "type": "aggregate",
          "groupby": ["Country"],
          "ops": ["q1", "q3", "median", "min", "max"],
          "fields": ["Wage", "Wage", "Wage", "Wage", "Wage"],
          "as": [
            "lower_box_Wage",
            "upper_box_Wage",
            "mid_box_Wage",
            "lower_whisker_Wage",
            "upper_whisker_Wage"
          ]
        }
      ]
    },
    {
      "name": "data_0",
      "source": "source_0",
      "transform": [
        {
          "type": "filter",
          "expr": "isValid(datum[\"lower_whisker_Wage\"]) && isFinite(+datum[\"lower_whisker_Wage\"])"
        }
      ]
    },
    {
      "name": "data_1",
      "source": "source_0",
      "transform": [
        {
          "type": "filter",
          "expr": "isValid(datum[\"upper_box_Wage\"]) && isFinite(+datum[\"upper_box_Wage\"])"
        }
      ]
    },
    {
      "name": "data_2",
      "source": "source_0",
      "transform": [
        {
          "type": "filter",
          "expr": "isValid(datum[\"lower_box_Wage\"]) && isFinite(+datum[\"lower_box_Wage\"])"
        }
      ]
    },
    {
      "name": "data_3",
      "source": "source_0",
      "transform": [
        {
          "type": "filter",
          "expr": "isValid(datum[\"mid_box_Wage\"]) && isFinite(+datum[\"mid_box_Wage\"])"
        }
      ]
    }
  ],
  "signals": [
    {"name": "x_step", "value": 20},
    {"name": "width", "update": "bandspace(domain('x').length, 0, 0) * x_step"}
  ],
  "marks": [
    {
      "name": "layer_0_marks",
      "type": "rule",
      "style": ["rule", "boxplot-rule"],
      "aria": false,
      "from": {"data": "data_0"},
      "encode": {
        "update": {
          "stroke": {"value": "black"},
          "tooltip": {
            "signal": "{\"Max of Minimum Wage\": format(datum[\"upper_whisker_Wage\"], \"\"), \"Q3 of Minimum Wage\": format(datum[\"upper_box_Wage\"], \"\"), \"Median of Minimum Wage\": format(datum[\"mid_box_Wage\"], \"\"), \"Q1 of Minimum Wage\": format(datum[\"lower_box_Wage\"], \"\"), \"Min of Minimum Wage\": format(datum[\"lower_whisker_Wage\"], \"\"), \"Country\": isValid(datum[\"Country\"]) ? datum[\"Country\"] : \"\"+datum[\"Country\"]}"
          },
          "x": {"scale": "x", "field": "Country", "band": 0.5},
          "y": {"scale": "y", "field": "lower_whisker_Wage"},
          "y2": {"scale": "y", "field": "lower_box_Wage"}
        }
      }
    },
    {
      "name": "layer_1_marks",
      "type": "rule",
      "style": ["rule", "boxplot-rule"],
      "aria": false,
      "from": {"data": "data_1"},
      "encode": {
        "update": {
          "stroke": {"value": "black"},
          "tooltip": {
            "signal": "{\"Max of Minimum Wage\": format(datum[\"upper_whisker_Wage\"], \"\"), \"Q3 of Minimum Wage\": format(datum[\"upper_box_Wage\"], \"\"), \"Median of Minimum Wage\": format(datum[\"mid_box_Wage\"], \"\"), \"Q1 of Minimum Wage\": format(datum[\"lower_box_Wage\"], \"\"), \"Min of Minimum Wage\": format(datum[\"lower_whisker_Wage\"], \"\"), \"Country\": isValid(datum[\"Country\"]) ? datum[\"Country\"] : \"\"+datum[\"Country\"]}"
          },
          "x": {"scale": "x", "field": "Country", "band": 0.5},
          "y": {"scale": "y", "field": "upper_box_Wage"},
          "y2": {"scale": "y", "field": "upper_whisker_Wage"}
        }
      }
    },
    {
      "name": "layer_2_marks",
      "type": "rect",
      "style": ["bar", "boxplot-box"],
      "from": {"data": "data_2"},
      "encode": {
        "update": {
          "ariaRoleDescription": {"value": "box"},
          "fill": {"scale": "color", "field": "Country"},
          "tooltip": {
            "signal": "{\"Max of Minimum Wage\": format(datum[\"upper_whisker_Wage\"], \"\"), \"Q3 of Minimum Wage\": format(datum[\"upper_box_Wage\"], \"\"), \"Median of Minimum Wage\": format(datum[\"mid_box_Wage\"], \"\"), \"Q1 of Minimum Wage\": format(datum[\"lower_box_Wage\"], \"\"), \"Min of Minimum Wage\": format(datum[\"lower_whisker_Wage\"], \"\"), \"Country\": isValid(datum[\"Country\"]) ? datum[\"Country\"] : \"\"+datum[\"Country\"]}"
          },
          "description": {
            "signal": "\"Country: \" + (isValid(datum[\"Country\"]) ? datum[\"Country\"] : \"\"+datum[\"Country\"]) + \"; Minimum Wage: \" + (format(datum[\"lower_box_Wage\"], \"\")) + \"; upper_box_Wage: \" + (format(datum[\"upper_box_Wage\"], \"\")) + \"; Max of Minimum Wage: \" + (format(datum[\"upper_whisker_Wage\"], \"\")) + \"; Q3 of Minimum Wage: \" + (format(datum[\"upper_box_Wage\"], \"\")) + \"; Median of Minimum Wage: \" + (format(datum[\"mid_box_Wage\"], \"\")) + \"; Q1 of Minimum Wage: \" + (format(datum[\"lower_box_Wage\"], \"\")) + \"; Min of Minimum Wage: \" + (format(datum[\"lower_whisker_Wage\"], \"\"))"
          },
          "xc": {"scale": "x", "field": "Country", "band": 0.5},
          "width": {"value": 14},
          "y": {"scale": "y", "field": "lower_box_Wage"},
          "y2": {"scale": "y", "field": "upper_box_Wage"}
        }
      }
    },
    {
      "name": "layer_3_marks",
      "type": "rect",
      "style": ["tick", "boxplot-median"],
      "aria": false,
      "from": {"data": "data_3"},
      "encode": {
        "update": {
          "opacity": {"value": 0.7},
          "fill": {"value": "white"},
          "tooltip": {
            "signal": "{\"Max of Minimum Wage\": format(datum[\"upper_whisker_Wage\"], \"\"), \"Q3 of Minimum Wage\": format(datum[\"upper_box_Wage\"], \"\"), \"Median of Minimum Wage\": format(datum[\"mid_box_Wage\"], \"\"), \"Q1 of Minimum Wage\": format(datum[\"lower_box_Wage\"], \"\"), \"Min of Minimum Wage\": format(datum[\"lower_whisker_Wage\"], \"\"), \"Country\": isValid(datum[\"Country\"]) ? datum[\"Country\"] : \"\"+datum[\"Country\"]}"
          },
          "xc": {"scale": "x", "field": "Country", "band": 0.5},
          "yc": {"scale": "y", "field": "mid_box_Wage"},
          "width": {"value": 14},
          "height": {"value": 1}
        }
      }
    }
  ],
  "scales": [
    {
      "name": "x",
      "type": "band",
      "domain": {
        "fields": [
          {"data": "data_0", "field": "Country"},
          {"data": "data_1", "field": "Country"},
          {"data": "data_2", "field": "Country"},
          {"data": "data_3", "field": "Country"}
        ],
        "sort": true
      },
      "range": {"step": {"signal": "x_step"}},
      "paddingInner": 0,
      "paddingOuter": 0
    },
    {
      "name": "y",
      "type": "linear",
      "domain": {
        "fields": [
          {"data": "data_0", "field": "lower_whisker_Wage"},
          {"data": "data_0", "field": "lower_box_Wage"},
          {"data": "data_1", "field": "upper_box_Wage"},
          {"data": "data_1", "field": "upper_whisker_Wage"},
          {"data": "data_2", "field": "lower_box_Wage"},
          {"data": "data_2", "field": "upper_box_Wage"},
          {"data": "data_3", "field": "mid_box_Wage"}
        ]
      },
      "range": [{"signal": "height"}, 0],
      "nice": true,
      "zero": true
    },
    {
      "name": "color",
      "type": "ordinal",
      "domain": {"data": "data_2", "field": "Country", "sort": true},
      "range": "category"
    }
  ],
  "axes": [
    {
      "scale": "y",
      "orient": "left",
      "gridScale": "x",
      "grid": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "domain": false,
      "labels": false,
      "aria": false,
      "maxExtent": 0,
      "minExtent": 0,
      "ticks": false,
      "zindex": 0
    },
    {
      "scale": "x",
      "orient": "bottom",
      "grid": false,
      "title": "Country",
      "labelAlign": "right",
      "labelAngle": 270,
      "labelBaseline": "middle",
      "zindex": 0
    },
    {
      "scale": "y",
      "orient": "left",
      "grid": false,
      "title": "Minimum Wage",
      "labelOverlap": true,
      "tickCount": {"signal": "ceil(height/40)"},
      "zindex": 0
    }
  ],
  "legends": [{"title": "Country", "fill": "color", "symbolType": "square"}],
  "config": {}
}